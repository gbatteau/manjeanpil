<%= form_for @profile, url: user_profile_path do |f| %>
  <div class="form-group">
    <%= f.label :restaurant_name %>
    <%= f.text_field :restaurant_name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :avatar %>
    <%= f.file_field :avatar, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :city %>
    <%= f.text_field :city, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.text_field :address, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :state %>
    <%= f.text_field :state, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :cuisine %>
    <%= f.text_field :cuisine, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :website %>
    <%= f.text_area :website, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :hours %>
    <%= f.text_area :hours, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :phone_number %>
    <%= f.text_area :phone_number, class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <%= f.submit "Update Profile", class: 'btn btn-primary' %>
  </div>
<% end %>
