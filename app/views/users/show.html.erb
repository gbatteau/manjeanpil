<div class="container">
  <div class="row">
    <div class="col-md-3 text-center">
      <%= image_tag @user.profile.avatar.url, class: 'avatar-img' %>
    </div>
    
    <div class="col-md-6">
      
      
      <div class="user-wrapper">
        <h1 class="username"><%= @user.profile.restaurant_name %></h1>
          <a class="edit-profile">
            <button>Edit Profile</button>
          </a>
      </div>
        <div class="well">
          <%= @user.profile.address %>
          <%= @user.profile.city %><br/>
          <%= @user.profile.state %><br/>
          <%= @user.profile.cuisine %><br/>
          <%= @user.profile.website %><br/>
          <%= @user.profile.hours %><br/>
          <%= @user.profile.phone_number %>
          
        </div>
      </div>
      
    </div>
  </div>
</div>    
<div class="user-tabs">
  <a class="tab active" href="">
    <i class="fa fa-th"></i>
    POSTS
  </a>
  <a class="tab" href="">
    <i class="fa fa-tv"></i>
    IGTV
  </a>
  <a class="tab" href="">
    <i class="fa fa-bookmark"></i>
    SAVED
  </a>
  <a class="tab" href="">
    <i class="fa fa-tag"></i>
    TAGGED
  </a>
</div>

<!-- for Picture post underneath profile -->
<div class="user-images">
  <div class="wrapper">
    <img src="your_image_url">
  </div>
  <div class="wrapper">
    <img src="your_image_url">
  </div>
  <div class="wrapper">
    <img src="your_image_url">
  </div>
</div>
